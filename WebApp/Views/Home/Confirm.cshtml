@model WebApp.Core.EF.RegisterViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>NAN 2019 | Đăng ký</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link href="@Url.Content("~/Content/LTE/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <!-- Font Awesome -->
    <link href="@Url.Content("~/Content/LTE/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <!-- Ionicons -->
    <link href="@Url.Content("~/Content/LTE/ionicons.min.css")" rel="stylesheet" type="text/css" />
    <!-- Theme style -->
    <link href="@Url.Content("~/Content/LTE/AdminLTE.min.css")" rel="stylesheet" type="text/css" />
    <!-- iCheck -->
    <link href="@Url.Content("~/Content/LTE/blue.css")" rel="stylesheet" type="text/css" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition login-page">
    <div class="login-box">
        <div class="login-box-body">
            <p class="login-box-msg text-bold text-success">Đăng ký thành công!</p>

            <div class="row">
                <div class="col-md-12">
                    <span class="text-center">Vui lòng kiểm tra Hộp thư đến email của bạn, một email xác nhận được gửi đến <b class="text-bold text-primary">@ViewBag.Email</b></span>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
